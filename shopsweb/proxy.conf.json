{
    "/api/*": {
        "target": "http://localhost:8080",
        "changeOrigin": true,
        "secure": false,
        "pathRewrite": {
            "^/api": ""
        }
    },
    "/*": {
        "target": "http://localhost:4200",
        "changeOrigin": true,
        "secure": false,
        "logLevel": "debug",
        "pathRewrite": {
            "^/": ""
        }
    }
}
